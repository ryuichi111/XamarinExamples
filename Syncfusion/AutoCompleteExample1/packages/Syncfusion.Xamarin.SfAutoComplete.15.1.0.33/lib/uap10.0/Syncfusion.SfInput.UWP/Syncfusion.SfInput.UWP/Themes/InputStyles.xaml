                                   Text="{TemplateBinding Expression}"
                                   TextAlignment="Right"
                                   TextWrapping="Wrap" />
                        <Viewbox Grid.Row="1"
                                 Grid.Column="1"
                                 HorizontalAlignment="Right"
                                 VerticalAlignment="Bottom">
                            <TextBlock Margin="8" Foreground="{TemplateBinding Foreground}"
                                       VerticalAlignment="Center"
                                       FontSize="50"
